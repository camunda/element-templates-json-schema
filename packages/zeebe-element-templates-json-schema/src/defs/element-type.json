{
  "allOf": [
    {
      "properties": {
        "eventDefinition": {
          "$id": "#/elementType/eventDefinition",
          "description": "If templating a message event, set this key's value to \"bpmn:MessageEventDefinition\". If templating a signal event, set this key's value to \"bpmn:SignalEventDefinition\". Otherwise, ignore.",
          "enum": [
            "bpmn:MessageEventDefinition",
            "bpmn:SignalEventDefinition"
          ]
        }
      }
    },
    {
      "if": {
        "required": [
          "eventDefinition"
        ]
      },
      "then": {
        "properties": {
          "value": {
            "enum": [
              "bpmn:StartEvent",
              "bpmn:IntermediateCatchEvent",
              "bpmn:IntermediateThrowEvent",
              "bpmn:BoundaryEvent",
              "bpmn:EndEvent"
            ]
          }
        },
        "required": [
          "value"
        ]
      }
    }
  ]
}
